{
    "compilerOptions":  {
                            "target":  "es2020",
                            "module":  "commonjs",
                            "moduleResolution":  "node",
                            "esModuleInterop":  true,
                            "strict":  true,
                            "resolveJsonModule":  true,
                            "outDir":  "dist",
                            "types":  [
                                          "hardhat"
                                      ],
                            "skipLibCheck":  true
                        },
    "include":  [
                    "./scripts",
                    "./deploy",
                    "./test",
                    "./hardhat.config.ts"
                ],
    "exclude":  [
                    "node_modules"
                ]
}
